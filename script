ME.Chatted:Connect(function(msg)
   if string.sub(msg, 1, 11) == prefix.."supermute " then
       for _, v in pairs(FindPlayer(string.sub(msg, 12))) do
           if v:FindFirstChild("bad_MutedNoob") then
               v["bad_MutedNoob"]:Destroy()
           end

           wait(0.1)
           local value = Instance.new("BoolValue",v)
           value.Name = "bad_MutedNoob"
       end
   end
end)

ME.Chatted:Connect(function(msg)
   if string.sub(msg, 1, 13) == prefix.."unsupermute " then
       for _, v in pairs(FindPlayer(string.sub(msg, 14))) do
           if v:FindFirstChild("bad_MutedNoob") then
               v:FindFirstChild("bad_MutedNoob"):Destroy()
           end
       end
   end
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
if msg:sub(1, 6) == "/goto " then
    local player
    for i, plr in ipairs(game.Players:GetPlayers()) do
    if string.lower(plr.Name):sub(1, string.len(msg:sub(7))) == string.lower(msg:sub(7)) then
        player = plr.Name
    end
    end
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace[player].HumanoidRootPart.CFrame
end
end)


while game:GetService("RunService").Heartbeat:wait() do
   for i,v in next, game:GetService("Players"):GetPlayers() do
       if v:FindFirstChild("bad_MutedNoob") then
           for i,v in pairs(v.Character:GetDescendants()) do
              if v:IsA("Tool") and v.Name == "BoomBox" then
                  v.Handle.Sound.Playing = false
              end
              end
       end
   end
end

local Player = game.Players.LocalPlayer
local plr = Player
local Character = plr.Character
local char = Character
local getto = char:GetChildren()
local bacpac = plr.Backpack
local backpacko = bacpac:GetChildren()
local radios = {}

for k in pairs(radios) do
    radios[k] = nil
end

function justdoit(robuxo, rabaxo, tabelo)
    for _, radz in pairs(robuxo) do
        if radz:IsA("Tool") then
            if radz.Handle:FindFirstChild("Sound") then
                table.insert(tabelo, radz.Handle)
            end
        end
    end
    for _, radz in pairs(rabaxo) do
        if radz:IsA("Tool") then
            if radz.Handle:FindFirstChild("Sound") then
                table.insert(tabelo, radz.Handle)
            end
        end
    end
end

function play(radzs)
    spawn(
        function()
            for i, v in pairs(radios) do
                if v.Sound.Playing == false then
                    v.Sound.Playing = true
                end
            end
        end
    )
end

justdoit(getto, backpacko, radios)

ME.Chatted:Connect(function(msg)
   if string.sub(msg, 1, 21) == prefix.."sync" then
       for _, v in pairs(FindPlayer(string.sub(msg, 22))) do
        play(radios)
        
        for i = 1, #radios do
            repeat wait() until radios[i].Sound.IsLoaded
        end
        
        spawn(
            function()
                for i = 1, #radios do
                    spawn(
                        function()
                    		radios[i].Sound.TimePosition = 0
                        end
                    )
                end
            end
        )
       end
end
end)
